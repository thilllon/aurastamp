{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "format": "prettier -w -l .",
    "build": "pnpm build:css && remix build",
    "dev": "concurrently --names \"CSS,REMIX\" -c \"bgBlue.bold,magenta.bold\" \"pnpm dev:css\" \"remix dev\"",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "build:css": "tailwindcss --input ./app/globals.css --output ./app/tailwind.css  --minify",
    "dev:css": "tailwindcss --input ./app/globals.css --output ./app/tailwind.css --watch"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/themes": "^1.1.2",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "firebase": "^10.3.1",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1",
    "isbot": "^3.6.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^10.1.8",
    "remix-google-cloud-functions": "^0.1.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "firebase-tools": "^12.5.2",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
