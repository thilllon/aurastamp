    "types": "prettier --list-different --write ./types/**",
    "predev": "yarn types && kill-port 3333",
    "dev": "next -p 3333",
    "prebuild": "yarn",
    "build": "next build",
    "postbuild": "next-sitemap && yarn prettier --write ./public/*.xml --config .prettierrc.sitemap.json",
    "start": "next start -p 3333",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --list-different --write .",
    "bs": "yarn types && yarn build && yarn start",
    "transform": "node transforms/runner && yarn format",
    "prepare_": "cd .. && husky install ./client/.husky",
    "test": "playwright test",
    "test:e2e": "playwright test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json,html,sh,xml}": [
      "prettier --list-different --write"
    ]
